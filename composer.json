{
    "name": "mohammadv184/arvancloud",
    "description": "PHP library for the ArvanCloud API",
    "keywords": [
        "arvan",
        "arvancloud",
        "Cloud",
        "php",
        "API",
        "php arvancloud",
        "cdn",
        "vod",
        "iaas",
        "Vads"
    ],
    "homepage": "https://github.com/mohammadv184/arvancloud",
    "authors": [
        {
            "name": "Mohammad Abbasi",
            "email": "mohammad.v184@gmail.com",
            "homepage": "https://github.com/mohammadv184",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "guzzlehttp/guzzle": "^7.3",
        "ext-json": "*",
        "nesbot/carbon": "^2.50"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4",
        "squizlabs/php_codesniffer": "3.*"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mohammadv184\\ArvanCloud\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mohammadv184\\ArvanCloud\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "config": {
        "sort-packages": true
    }
}
